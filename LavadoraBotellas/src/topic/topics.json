        "name": "lavado_gaseosa",
        "permission" : ["lavarGaseosa","secarGaseosa"],
        "fireCallback" : ["finSecadoGaseosa"]
    },
    {
        "name" : "expulsion_otra",
        "permission": ["expulsarBotella"],
        "fireCallback" : ["finExpulsion"]
    }
]